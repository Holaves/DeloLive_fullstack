{
  "name": "redux-typescript",
  "version": "1.2.1",
  "description": "redux-typescript",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf ./.d.ts ./lib ./temp ./dist",
    "transform": "tsc",
    "declare": "mkdir ./.d.ts && typings bundle --out ./.d.ts/redux-typescript.d.ts",
    "browser": "mkdir ./dist && browserify ./lib/index.js --standalone redux-typescript --outfile ./dist/redux-typescript.js",
    "min": "uglifyjs ./dist/redux-typescript.js --compress --mangle > ./dist/redux-typescript.min.js",
    "build": "npm run clean && npm run transform && npm run declare && npm run browser && npm run min",
    "test": "npm run build && webpack-simple-server -d test/ -r '\\.(tsx?|jsx?)$' -c webpack.config.js",
    "test:mocha": "clear && cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrazing/redux-typescript.git"
  },
  "keywords": [
    "redux-typescript"
  ],
  "author": "acrazing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acrazing/redux-typescript/issues"
  },
  "homepage": "https://github.com/acrazing/redux-typescript#readme",
  "dependencies": {}
}
